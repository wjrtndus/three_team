--  DDL for Trigger TERMS_TYPE_TRG
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "C##TEAM3"."TERMS_TYPE_TRG" 
BEFORE INSERT ON TERMS_TYPE 
FOR EACH ROW 
BEGIN
  <<COLUMN_SEQUENCES>>
  BEGIN
    IF INSERTING AND :NEW.TERMS_TYPE_IDX IS NULL THEN
      SELECT TERMS_TYPE_SEQ.NEXTVAL INTO :NEW.TERMS_TYPE_IDX FROM SYS.DUAL;
    END IF;
  END COLUMN_SEQUENCES;
END;
